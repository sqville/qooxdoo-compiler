    "replace-in-file": "^5.0.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2",
    "set-value": "^3.0.2",
    "showdown": "^1.9.1",
    "source-map": "^0.5.7",
    "tmp": "0.2.1",
    "uglify-es": "^3.3.9",
    "unset-value": "^1.0.0",
    "upath": "^1.2.0",
    "update-notifier": "^4.1.1",
    "xml2js": "^0.4.23",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@qooxdoo/compiler": "^1.0.0-beta",
    "source-map-support": "^0.5.19",
    "tape": "^5.0.1"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "@qooxdoo/qx"
    ]
  },
  "engine": "node >= 8"
}
